{
  "name": "ts-monorepo-template",
  "private": true,
  "packageManager": "pnpm@7.14.0",
  "scripts": {
    "dev": "ts-node ./scripts/tsc-watch --onSuccess \\\"node build\\\"",
    "build": "ts-node ./scripts/tsc.ts",
    "watch": "ts-node ./scripts/tsc.ts -w",
    "create:module": "ts-node ./scripts/create-module.ts"
  },
  "dependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.13",
    "tsc-watch": "^6.0.0"
  }
}
